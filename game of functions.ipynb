    "def sum_list(nums):\n",
    "  sum = 0\n",
    "\n",
    "  for num in nums:\n",
    "    sum += num\n",
    "\n",
    "  return sum\n",
    "\n",
    "nums = input(\"Enter a list of numbers seperated by comma: \")\n",
    "\n",
    "nums = list(map(int, nums.split()))\n",
    "sum = sum_list(nums)\n",
    "\n",
    "print(sum)"
   ]
  }
