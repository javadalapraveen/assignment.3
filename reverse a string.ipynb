    "def reverse_string(string):\n",
    " \n",
    "  reversed_string = \"\"\n",
    "\n",
    "  for i in range(len(string) - 1, -1, -1):\n",
    "    reversed_string += string[i]\n",
    "\n",
    "  return reversed_string\n",
    "\n",
    "string = input(\"Enter a string: \")\n",
    "\n",
    "reversed_string = reverse_string(string)\n",
    "\n",
    "print(reversed_string)"
   ]
  }
